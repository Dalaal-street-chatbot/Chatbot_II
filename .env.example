# Dalaal Street Chatbot - Environment Variables Example
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8000
DEBUG=True
ENVIRONMENT=development

# AI Service API Keys
GROQ_API_KEY=your_groq_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
CODESTRAL_API_KEY=your_codestral_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
OLLAMA_API_URL=http://localhost:11434/api

# Stock Market Data APIs
UPSTOX_API_KEY=your_upstox_api_key_here
UPSTOX_ACCESS_TOKEN=your_upstox_access_token_here
INDIAN_STOCK_API_KEY=your_indian_stock_api_key_here

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_AI_API_KEY=your_gemini_api_key_here
VERTEX_AI_API_CLIENT_ID=your_vertex_client_id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your-credentials.json

# Azure AI Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_key
AZURE_EXISTING_AIPROJECT_ENDPOINT=https://your-azure-endpoint.openai.azure.com/

# Financial Data APIs
NEWS_API=your_newsapi_key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key
UPSTOX_API_KEY=your_upstox_key

# Database Configuration
DATABASE_URL=postgresql://dbuser:dbpassword@postgres:5432/dalaalstreetbot
REDIS_URL=redis://redis:6379/0
